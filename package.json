{
  "name": "music-player",
  "version": "1.0.0",
  "description": "app",
  "main": "./src/index",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env WATCH_MODE=true ts-node-dev --ignore-watch node_modules --files --project tsconfig.json src/index.ts",
    "build": "if [ \"$NODE_ENV\" = \"development\" ]; then npm run build:dev ; else npm run build:prod; fi",
    "build:dev": "tsc --project tsconfig.production.json && cross-env ANALYSE_BUNDLE=true webpack --config=webpack.config.js --progress",
    "build:prod": "tsc --project tsconfig.production.json && cross-env NODE_ENV=production webpack --config=webpack.production.config.js --progress",
    "clean": "rm -rf cache dist tmp ts-node",
    "test:unit": "yarn prettier && yarn lint && yarn test:unit:typecheck && yarn test:unit:logic && yarn test:unit:ui",
    "test:unit:logic": "ts-mocha --recursive -p tsconfig.json \"./test/unit/**/*.spec.ts\"",
    "test:unit:ui": "ts-mocha --recursive -p tsconfig.json \"./test/unit/**/*.spec.tsx\"",
    "prettier": "prettier --check \"**/*.ts?(x)\"",
    "prettier:fix": "prettier --write --check \"**/*.ts?(x)\"",
    "lint": "eslint --color --ext .tsx,.ts . ",
    "lint:fix": "eslint --color --ext .tsx,.ts . --fix",
    "lint:prettier:fix": "yarn prettier:fix && yarn lint:fix",
    "test:unit:typecheck": "tsc  --p tsconfig.unit.json --noEmit",
    "tslint": "tslint -c ./tslint.json --project ./tsconfig.json --format stylish",
    "stats": "webpack --profile --json --config=webpack.production.config.js > stats.json",
    "ci": "npm run build:prod && git add . && git commit -m \"ci\" && git push"
  },
  "author": "",
  "license": "ISC",
